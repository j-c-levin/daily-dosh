{
  "name": "daily-dosh",
  "version": "1.0.0",
  "description": "a monzo connected webapp to help budget daily",
  "main": "server/index.js",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "claudia:setup": "npm-run-all compile claudia:express:setup",
    "compile": "tsc",
    "claudia:express:setup": "claudia generate-serverless-express-proxy --express-module dist/index"
  },
  "repository": "git+https://github.com/j-c-levin/daily-dosh.git",
  "keywords": [
    "monzo",
    "budget",
    "angular"
  ],
  "author": "joshua levin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/j-c-levin/daily-dosh/issues"
  },
  "homepage": "https://github.com/j-c-levin/daily-dosh#readme",
  "dependencies": {
    "@angular/cli": "^6.1.2",
    "aws-sdk": "^2.298.0",
    "body-parser": "^1.18.3",
    "claudia": "^5.1.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "superagent": "^3.8.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.6",
    "@types/superagent": "^3.8.3",
    "@types/uuid": "^3.4.3",
    "nodemon": "^1.18.3",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "npm-run-all": "^4.1.3"
  }
}
